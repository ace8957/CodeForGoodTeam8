<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.handsoncentralohio"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.handsoncentralohio.MainLayoutActivity"
            android:label="Hands On" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.handsoncentralohio.EventsActivity"
            android:label="Hands On"
            android:parentActivityName="com.example.handsoncentralohio.MainLayoutActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.handsoncentralohio.MainLayoutActivity" />
        </activity>
        <activity
            android:name="com.example.handsoncentralohio.LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
    </application>

</manifest>
